Config { bgColor = "#333333"
       , fgColor = "grey"
       , position = TopW L 100
       , commands = [ Run Cpu ["-L", "3", "-H", "50", "--normal", "green", "--high", "red"] 10
                    , Run Battery ["-t", "bat: <left>%", "Bat0"] 600
                    , Run Date "%Y-%m-%d %H:%M" "date" 10
                    , Run StdinReader]
       , sepChar = "%"
       , alignSep = "}{"
       , template = "%StdinReader% }{ %date% | %battery% | %cpu%"
       }
